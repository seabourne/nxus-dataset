{
  "name": "nxus-dataset",
  "version": "4.0.0-2",
  "description": "DataSet-DataRow data module ",
  "main": "lib",
  "scripts": {
    "preversion": "npm test",
    "postversion": "npm run build-docs && git push && git push --tags",
    "test": "NODE_ENV=test mocha --compilers js:babel-register -R spec test/*.js ",
    "compile": "babel src --out-dir lib; npm run copyTemplates",
    "copyTemplates": " cp -r src/templates lib/templates; ",
    "prepublish": "npm run compile",
    "build-docs": "export NAME=`npm view . name`; export VERSION=`npm view . version`; documentation readme ./src/*.js --name $NAME --project-version $VERSION --readme-file ./README.md -s $NAME",
    "publish-docs": "npm run build-docs && git add ./README.md && git commit -m 'Updated README API Docs' && git push"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/seabourne/nxus-dataset.git"
  },
  "author": "scottmaxson",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/seabourne/nxus-dataset/issues"
  },
  "homepage": "https://github.com/seabourne/nxus-dataset",
  "babel": {
    "presets": [
      "es2015"
    ]
  },
  "dependencies": {
    "bluebird": "^3.3.3",
    "morph": "^0.2.0",
    "node-uuid": "^1.4.7",
    "nxus-admin": "^4.0.0-2",
    "nxus-data-manager": "^4.0.0-1",
    "nxus-router": "^4.0.0-1",
    "nxus-storage": "^4.0.0-2",
    "nxus-templater": "^4.0.0-1",
    "nxus-web": "^4.0.0-4",
    "underscore": "^1.8.3"
  },
  "devDependencies": {
    "documentation": "^4.0.0-beta9",
    "chai": "^3.4.1",
    "chai-as-promised": "^5.1.0",
    "mocha": "^2.2.5",
    "should": "^7.0.2",
    "sinon": "^1.17.2",
    "babel-plugin-transform-function-bind": "^6.8.0",
    "babel-plugin-transform-object-rest-spread": "^6.8.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-cli": "^6.9.0",
    "sails-memory": "^0.10.7",
    "babel-core": "^6.9.0"
  }
}
